{
  "name": "pk-ts-node-lib",
  "version": "l.0.1",
  "author": "Paul Kirkaas",
  "license": "UNLICENSED",
  "description": "TS/JS Utilities for node apps, including common JS functions",
  "main": "dist/index",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./tst-exports": {
      "types": "./dist/tst-exports.d.ts",
      "import": "./dist/tst-exports.js",
      "default": "./dist/tst-exports.js",
      "require": "./dist/tst-exports.js"
    },
    "./tsconfig.json": "./tsconfig.json"
  },
  "type": "commonjs",
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./node_modules package-lock.json"
  },
  "dependencies": {
    "pk-ts-common-lib": "git+https://github.com/pkirkaas/pk-ts-common-lib.git#working",
    "inquirer": "latest",
    "typescript":"*",
    "yargs": "latest",
    "stack-trace": "*",
    "fs-path": "*",
    "fs-extra": "*",
    "dotenv": "*",
    "stacktracey": "*",
    "ts-node": "*",
    "tsconfig-paths": "*",
    "uuid": "*"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/lodash": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "*",
    "prettier": "*"
  }
}
