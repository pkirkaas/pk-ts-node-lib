{
  "name": "pk-ts-node-lib",
  "version": "l.0.1",
  "author": "Paul Kirkaas",
  "license": "UNLICENSED",
  "description": "TS/JS Utilities for node apps, including common JS functions",
  "engines": {
    "node": ">= 18.0.0"
  },
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./tsconfig.cjs.json": "./tsconfig.cjs.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && tsc -p ./tsconfig.cjs.json",
    "clean": "rm -rf ./dist ./node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@tsconfig/node18": "^18.2.1",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "dotenv": "*",
    "fs-extra": "*",
    "fs-path": "*",
    "inquirer": "latest",
    "pk-ts-common-lib": "git+https://github.com/pkirkaas/pk-ts-common-lib.git#working",
    "ts-node": "*",
    "tsconfig-paths": "*",
    "typescript": "^5.2.2",
    "uuid": "*",
    "valid-filename": "^4.0.0",
    "yargs": "latest"
  }
}
